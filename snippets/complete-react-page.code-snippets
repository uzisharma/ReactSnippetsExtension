  "Dashboard Page Component": {
    "prefix": "dashboardpage",
    "body": [
      "import { useEffect, useState } from \"react\";",
      "import { useNavigate } from \"react-router\";",
      "",
      "const ${TM_FILENAME_BASE} = () => {",
      "  const navigate = useNavigate();",
      "  const STORAGE_KEY = \"${1:rememberedUser}\";",
      "  const [user, setUser] = useState(null);",
      "",
      "  useEffect(() => {",
      "    try {",
      "      const raw = localStorage.getItem(STORAGE_KEY);",
      "      if (!raw) {",
      "        navigate(\"${2:/login}\");",
      "        return;",
      "      }",
      "      const parsed = JSON.parse(raw);",
      "      setUser(parsed);",
      "    } catch (e) {",
      "      console.error(\"Invalid user in storage\", e);",
      "      navigate(\"${2:/login}\");",
      "    }",
      "  }, [navigate]);",
      "",
      "  const onLogout = () => {",
      "    localStorage.removeItem(STORAGE_KEY);",
      "    navigate(\"${3:/}\");",
      "  };",
      "",
      "  // Example: fetch dashboard data after mount",
      "  // useEffect(() => {",
      "  //   // fetchData().then(setData);",
      "  // }, []);",
      "",
      "  if (!user) return null;",
      "",
      "  return (",
      "    <div className=\"dashboard-page\">",
      "      <header className=\"dashboard-header\">",
      "        <h1>${4:Dashboard}</h1>",
      "        <div className=\"dashboard-actions\">",
      "          <span>Welcome, {user.fullName || user.email}</span>",
      "          <button onClick={onLogout}>${5:Logout}</button>",
      "        </div>",
      "      </header>",
      "",
      "      <main className=\"dashboard-content\">",
      "        {/* Your dashboard widgets/components go here */}",
      "        <section className=\"card\">",
      "          <h2>${6:Overview}</h2>",
      "          <p>${7:This is your dashboard overview.}</p>",
      "        </section>",
      "      </main>",
      "    </div>",
      "  );",
      "};",
      "",
      "export default ${TM_FILENAME_BASE};"
    ],
    "description": "Dashboard page that loads user from localStorage, redirects if missing, and provides a logout handler."
  }
