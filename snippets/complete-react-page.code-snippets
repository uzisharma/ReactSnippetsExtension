  "Login Page Component": {
    "prefix": "loginpage",
    "body": [
      "import { useEffect, useState } from \"react\";",
      "import Form from \"../component/Form\";",
      "import { CheckBox } from \"../component/Input\";",
      "import { useNavigate } from \"react-router\";",
      "",
      "const ${TM_FILENAME_BASE} = ({ ${1:storedUserDetails} }) => {",
      "  const navigate = useNavigate();",
      "  const [errors, setErrors] = useState({});",
      "  const [${2:rememberMe}, set${2/(.*)/${1:/capitalize}/}] = useState(false);",
      "  const [${3:loginData}, set${3/(.*)/${1:/capitalize}/}] = useState({",
      "    email: \"\",",
      "    password: \"\",",
      "  });",
      "",
      "  useEffect(() => {",
      "    const savedUser = localStorage.getItem(\"${4:rememberedUser}\");",
      "    if (savedUser) {",
      "      const user = JSON.parse(savedUser);",
      "      set${3/(.*)/${1:/capitalize}/}({",
      "        email: user.email,",
      "        password: user.password,",
      "      });",
      "    }",
      "  }, [${2:rememberMe}]);",
      "",
      "  const inputFields = [",
      "    {",
      "      name: \"email\",",
      "      label: \"email\",",
      "      type: \"email\",",
      "      placeholder: \"example@gmail.com\",",
      "    },",
      "    {",
      "      name: \"password\",",
      "      label: \"Password\",",
      "      type: \"password\",",
      "      placeholder: \"Enter your password\",",
      "    },",
      "  ];",
      "",
      "  const validateField = (name, value) => {",
      "    let error = \"\";",
      "",
      "    switch (name) {",
      "      case \"email\":",
      "        if (!value.trim()) error = \"Email is required\";",
      "        else if (!/\\\\S+@\\\\S+\\\\.\\\\S+/.test(value)) error = \"Invalid email\";",
      "        break;",
      "",
      "      case \"password\":",
      "        if (!value.trim()) error = \"Password is required\";",
      "        else if (value.length < 6)",
      "          error = \"Password should be atleast 6 character long\";",
      "        break;",
      "",
      "      default:",
      "        break;",
      "    }",
      "    return error;",
      "  };",
      "",
      "  const onChange = (name, value) => {",
      "    set${3/(.*)/${1:/capitalize}/}((prev) => ({ ...prev, [name]: value }));",
      "",
      "    const fieldError = validateField(name, value);",
      "    setErrors((prev) => ({ ...prev, [name]: fieldError }));",
      "  };",
      "",
      "  const validateForm = () => {",
      "    const newError = {};",
      "    for (const key in ${3:loginData}) {",
      "      const error = validateField(key, ${3:loginData}[key]);",
      "      if (error) newError[key] = error;",
      "    }",
      "    return newError;",
      "  };",
      "",
      "  const ${5:loginSubmitHandler} = (e) => {",
      "    e.preventDefault();",
      "    const validationForm = validateForm();",
      "    if (Object.keys(validationForm).length > 0) {",
      "      setErrors(validationForm);",
      "      return;",
      "    }",
      "",
      "    setErrors({});",
      "",
      "    const user = ${1:storedUserDetails}.find(",
      "      (u) => u.email === ${3:loginData}.email && u.password === ${3:loginData}.password",
      "    );",
      "",
      "    if (user) {",
      "      alert(\"Login Successful\");",
      "      navigate(\"${6:/dashboard}\");",
      "      if (${2:rememberMe}) {",
      "        localStorage.setItem(\"${4:rememberedUser}\", JSON.stringify(user));",
      "      } else {",
      "        localStorage.removeItem(\"${4:rememberedUser}\");",
      "      }",
      "    } else {",
      "      alert(\"Invalid Credentials\");",
      "    }",
      "  };",
      "",
      "  return (",
      "    <div className=\"login-page\">",
      "      <div className=\"container-box\">",
      "        <Form",
      "          heading={\"${7:Login}\"}",
      "          formData={${3:loginData}}",
      "          inputFields={inputFields}",
      "          onChange={onChange}",
      "          onSubmit={${5:loginSubmitHandler}}",
      "          errors={errors}",
      "        />",
      "        <div className=\"remember-me\">",
      "          <CheckBox",
      "            label={\"${8:Remember Me}\"}",
      "            name={\"rememberMe\"}",
      "            checked={${2:rememberMe}}",
      "            setChecked={set${2/(.*)/${1:/capitalize}/}}",
      "          />",
      "        </div>",
      "      </div>",
      "    </div>",
      "  );",
      "};",
      "",
      "export default ${TM_FILENAME_BASE};"
    ],
    "description": "Login page with linked placeholders for state, handler, storage key, and route. Component name auto-uses file name."
