{
  "Signup Page Component": {
    "prefix": "signuppage",
    "body": [
      "import { useState } from \"react\";",
      "import Form from \"../component/Form\";",
      "import { useNavigate } from \"react-router\";",
      "",
      "const ${TM_FILENAME_BASE} = ({ setStoredUser }) => {",
      "  const navigate = useNavigate();",
      "  const [errors, setErrors] = useState({});",
      "  const [${1:registerData}, set${1/(.*)/${1:/capitalize}/}] = useState({",
      "    fullName: \"\",",
      "    email: \"\",",
      "    password: \"\",",
      "  });",
      "",
      "  const inputFields = [",
      "    {",
      "      name: \"fullName\",",
      "      label: \"fullName\",",
      "      type: \"text\",",
      "      placeholder: \"Enter your full name\",",
      "    },",
      "    {",
      "      name: \"email\",",
      "      label: \"email\",",
      "      type: \"email\",",
      "      placeholder: \"example@gmail.com\",",
      "    },",
      "    {",
      "      name: \"password\",",
      "      label: \"Password\",",
      "      type: \"password\",",
      "      placeholder: \"Enter your password\",",
      "    },",
      "    {",
      "      name: \"confirm-password\",",
      "      label: \"Confirm-Password\",",
      "      type: \"password\",",
      "      placeholder: \"Confirm your password\",",
      "    },",
      "  ];",
      "",
      "  const validateField = (name, value) => {",
      "    let error = \"\";",
      "",
      "    switch (name) {",
      "      case \"fullName\":",
      "        if (!value.trim()) error = \"Full Name is required\";",
      "        break;",
      "      case \"email\":",
      "        if (!value.trim()) error = \"Email is required\";",
      "        else if (!/\\\\S+@\\\\S+\\\\.\\\\S+/.test(value)) error = \"Invalid email\";",
      "        break;",
      "",
      "      case \"password\":",
      "        if (!value.trim()) error = \"Password is required\";",
      "        else if (value.length < 6)",
      "          error = \"Password should be atleast 6 character long\";",
      "        break;",
      "      case \"confirm-password\":",
      "        if (value !== ${1:registerData}[\"password\"])",
      "          error = \"Password does not matches\";",
      "        break;",
      "",
      "      default:",
      "        break;",
      "    }",
      "    return error;",
      "  };",
      "",
      "  const onChange = (name, value) => {",
      "    set${1/(.*)/${1:/capitalize}/}((prev) => ({ ...prev, [name]: value }));",
      "",
      "    const fieldError = validateField(name, value);",
      "    setErrors((prev) => ({ ...prev, [name]: fieldError }));",
      "  };",
      "",
      "  const validateForm = () => {",
      "    const newError = {};",
      "    for (const key in ${1:registerData}) {",
      "      const error = validateField(key, ${1:registerData}[key]);",
      "      if (error) newError[key] = error;",
      "    }",
      "    return newError;",
      "  };",
      "",
      "  const signupHandler = (e) => {",
      "    e.preventDefault();",
      "    const validationForm = validateForm();",
      "    if (Object.keys(validationForm).length > 0) {",
      "      setErrors(validationForm);",
      "      return;",
      "    }",
      "",
      "    setErrors({});",
      "",
      "    setStoredUser((prev) => [...prev, ${1:registerData}]);",
      "    navigate(\"${3:/login}\");",
      "  };",
      "",
      "  return (",
      "    <div className=\"signup-page\">",
      "      <Form",
      "        heading={\"${2:Signup}\"}",
      "        formData={${1:registerData}}",
      "        inputFields={inputFields}",
      "        onChange={onChange}",
      "        onSubmit={signupHandler}",
      "        errors={errors}",
      "      />",
      "    </div>",
      "  );",
      "};",
      "",
      "export default ${TM_FILENAME_BASE};"
    ],
    "description": "Signup page with linked form state, validation, and navigation. Component name auto-uses file name."
